<resources>

    <style name="MatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="WrapMatch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="Match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="Wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="FloatingActionButtonTop" parent="Wrap">
        <item name="android:layout_margin">30dp</item>
        <item name="android:layout_gravity">right|top</item>
    </style>

    <style name="FloatingActionButtonBottom" parent="Wrap">
        <item name="android:layout_margin">30dp</item>
        <item name="android:layout_gravity">right|bottom</item>
    </style>

    <style name="MatchFill">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSContainer">
        <item name="android:background">@color/cs_white</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="CSNavigationContainer">
        <item name="android:background">@color/cs_white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">false</item>
    </style>

</resources>
